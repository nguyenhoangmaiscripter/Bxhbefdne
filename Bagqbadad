Section3:CreateButton("instant skill 1", function()
local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local tool = character:FindFirstChildOfClass("Tool")
    if tool and tool:FindFirstChild("1") then
        local remoteEvent = tool["1"]:FindFirstChild("RemoteEvent")
        if remoteEvent then
            remoteEvent:FireServer()
            print("Skill 1 ƒë√£ ƒë∆∞·ª£c k√≠ch ho·∫°t th√¥ng qua RemoteEvent!")
        else
            warn("Kh√¥ng t√¨m th·∫•y RemoteEvent trong Tool!")
        end
    else
        warn("Ng∆∞·ªùi ch∆°i ch∆∞a c·∫ßm d·ª•ng c·ª• ho·∫∑c kh√¥ng t√¨m th·∫•y Tool.")
